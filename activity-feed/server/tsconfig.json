{
    "compilerOptions": {
      "target": "es2015",
      "strict": false,
      "allowJs": true,
      "skipLibCheck": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
      "lib": [
          "es2015",
          "es2016",
          "es2017",
          "esnext"
      ],  
    },
    "include": [
      "src"
    ]
  }